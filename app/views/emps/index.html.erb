<h1>Listing emps</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Branch</th>
    <th>Phneno</th>
    <th>Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @emps.each do |emp| %>
  <tr>
    <td><%= emp.name %></td>
    <td><%= emp.branch %></td>
    <td><%= emp.phneno %></td>
    <td><%= emp.address %></td>
    <td><%= link_to 'Show', emp %></td>
    <td><%= link_to 'Edit', edit_emp_path(emp) %></td>
    <td><%= link_to 'Destroy', emp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Emp', new_emp_path %>
